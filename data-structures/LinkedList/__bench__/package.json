{
  "name": "@dsa/linkedlist_bench",
  "version": "1.0.0",
  "main": "./src/index.ts",
  "types": "./src/index.ts",
  "type": "module",
  "scripts": {
    "clean": "rm -rf .turbo node_modules",
    "test": "jest",
