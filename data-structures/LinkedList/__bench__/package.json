    "@dsa/linkedlist": "workspace:^",
    "@dsa/tsconfig": "workspace:^"
  },
  "jest": {
    "preset": "@dsa/jest-config"
  },
  "devDependencies": {
    "@jest/globals": "^29.7.0",
    "@js-sdsl/link-list": "^4.4.2",
    "esbuild": "^0.23.0",
    "jest": "^29.7.0",
    "tinybench": "^2.9.0",
    "ts-jest": "^29.2.4"
  }
}
